{
  "name": "more-human-internet-frontend",
  "description": "Frontend for morehumaninternet.org",
  "version": "0.1.0",
  "author": "Will Weiss <will.weiss1230@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --host=0.0.0.0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "lint": "{ find src -name '*.ts' -o -name '*.tsx' | xargs tslint --project tsconfig.json --config tslint.json ; }",
    "start": "netlify dev",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "mocha -r ts-node/register -r src/types.ts -r src/__tests/setup.ts src/**/*.test.ts*"
  },
  "dependencies": {
    "@material-ui/core": "4.10.2",
    "@material-ui/icons": "4.9.1",
    "@material-ui/styles": "4.10.0",
    "algoliasearch": "^4.5.1",
    "axios": "0.19.2",
    "color": "3.1.2",
    "lodash": "4.17.20",
    "react": "16.12.0",
    "react-axe": "3.5.3",
    "react-beautiful-dnd": "13.0.0",
    "react-dom": "16.12.0",
    "react-helmet": "6.1.0",
    "react-scroll-parallax": "2.3.4",
    "redux": "4.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/color": "3.0.1",
    "@types/mocha": "^8.0.3",
    "@types/react-beautiful-dnd": "13.0.0",
    "@types/react-helmet": "6.0.0",
    "@types/sinon": "^9.0.5",
    "@types/sinon-chai": "^3.2.4",
    "chai": "4.2.0",
    "gatsby": "2.23.3",
    "gatsby-image": "2.4.13",
    "gatsby-plugin-intl": "0.3.3",
    "gatsby-plugin-manifest": "2.4.11",
    "gatsby-plugin-material-ui": "2.1.9",
    "gatsby-plugin-react-axe": "0.4.1",
    "gatsby-plugin-react-helmet": "3.3.4",
    "gatsby-plugin-sass": "2.3.10",
    "gatsby-plugin-sharp": "2.6.21",
    "gatsby-plugin-sitemap": "2.4.11",
    "gatsby-source-filesystem": "2.3.21",
    "gatsby-transformer-sharp": "2.5.11",
    "mocha": "8.1.3",
    "netlify-cli": "2.54.0",
    "node-sass": "4.14.1",
    "prettier": "2.0.5",
    "sinon": "9.0.3",
    "sinon-chai": "^3.5.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "tslint-immutable": "6.0.1"
  },
  "keywords": [
    "human",
    "internet"
  ],
